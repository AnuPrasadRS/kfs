        @Html.TextBoxFor(a => a.pobothers)
    </td>
    <td style="font-family:Calibri;text-align:center;font-size:11px;border-left:1px solid;border-top:1px solid;border-bottom:1px solid;border-left-color:#000000;border-top-color:#000000;border-bottom-color:#000000;" colspan=7>

        @Html.TextBoxFor(a => a.remarksothers, new { @class = "report_remark" })
    </td>
</tr>
<tr style="height:28.8px;">
    <td style="font-family:Calibri;text-align:center;font-size:11px;font-weight:bold;border-left:2px solid;border-right:1px solid;border-top:1px solid;border-bottom:2px solid;border-left-color:#000000;border-right-color:#000000;border-top-color:#000000;border-bottom-color:#000000;" class="auto-style6">
        <nobr>Total</nobr>
    </td>
    <td style="font-family:Calibri;text-align:center;font-size:11px;border-left:1px solid;border-top:1px solid;border-bottom:2px solid;border-left-color:#000000;border-top-color:#000000;border-bottom-color:#000000;" colspan=2></td>
    <td style="font-family:Calibri;text-align:center;font-size:11px;font-weight:bold;border-left:1px solid;border-top:1px solid;border-bottom:2px solid;border-left-color:#000000;border-top-color:#000000;border-bottom-color:#000000;" colspan=2>
        @Html.TextBoxFor(a => a.pobtotal)
    </td>
    <td style="font-family:Calibri;text-align:center;font-size:11px;border-left:1px solid;border-top:1px solid;border-bottom:2px solid;border-left-color:#000000;border-top-color:#000000;border-bottom-color:#000000;" colspan=7>

        @Html.TextBox("RemarkTotal","", new { @class = "report_remark" })
    </td>
</tr>
<script>
    $(document).ready(function () {
        $('#pobcrew').focusout(function () {
            var value = $(this).val();
            var calc = value * 72 * 4;
            $('#manhours').val(calc);
            $('#manhourss').html(calc);

        });
    });
</script>