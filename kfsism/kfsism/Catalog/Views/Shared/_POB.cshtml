        <nobr>Charterers</nobr>
    </td>
    <td style="font-family:Calibri;text-align:center;font-size:11px;border-left:1px solid;border-top:1px solid;border-bottom:1px solid;border-left-color:#000000;border-top-color:#000000;border-bottom-color:#000000;" colspan=2>
        @Html.TextBoxFor(a => a.pobcharterers)
    </td>
    <td style="font-family:Calibri;text-align:center;font-size:11px;border-left:1px solid;border-top:1px solid;border-bottom:1px solid;border-left-color:#000000;border-top-color:#000000;border-bottom-color:#000000;" colspan=7>

        @Html.TextBoxFor(a => a.remarkscharterers, new { @class = "report_remark" })
    </td>
</tr>
<tr style="height:28.8px;">
    <td style="font-family:Calibri;text-align:center;font-size:11px;border-left:2px solid;border-right:1px solid;border-left-color:#000000;border-right-color:#000000;" class="auto-style6">
        <nobr>3</nobr>
    </td>
    <td style="font-family:Calibri;text-align:center;font-size:11px;border-left:1px solid;border-top:1px solid;border-left-color:#000000;border-top-color:#000000;" colspan=2>
        <nobr>KFS office staff</nobr>
    </td>
    <td style="font-family:Calibri;text-align:center;font-size:11px;border-left:1px solid;border-top:1px solid;border-bottom:1px solid;border-left-color:#000000;border-top-color:#000000;border-bottom-color:#000000;" colspan=2>

        @Html.TextBoxFor(a => a.pobkfc)
    </td>
    <td style="font-family:Calibri;text-align:center;font-size:11px;border-left:1px solid;border-top:1px solid;border-bottom:1px solid;border-left-color:#000000;border-top-color:#000000;border-bottom-color:#000000;" colspan=7>

        @Html.TextBoxFor(a => a.remarkskfc, new { @class = "report_remark" })
    </td>
</tr>
<tr style="height:28.8px;">
    <td style="font-family:Calibri;text-align:center;font-size:11px;border-left:2px solid;border-right:1px solid;border-top:1px solid;border-bottom:1px solid;border-left-color:#000000;border-right-color:#000000;border-top-color:#000000;border-bottom-color:#000000;" class="auto-style6">
        <nobr>4</nobr>
    </td>
